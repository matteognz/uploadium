{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "declaration": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "types": ["node"],
    "baseUrl": ".", 
    "paths": {
      "uploadium": ["./src"]
    }
  },
  "include": ["src"],
  "exclude": ["dist"]
}
